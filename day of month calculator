#include<stdio.h>
int main()
{
    int n;
    printf("Enter Number for month-");
    scanf("%d",&n);
    if(n==1)
    printf("31 days");
    else if(n==2)
    printf("28 days");
    else if(n==3)
    printf("31 days");
    else if(n==4)
    printf("30 days");
    else if(n==5)
    printf("31 days");
    else if(n==6)
    printf("30 days");
    else if(n==7)
    printf("31 days");
    else if(n==8)
    printf("31 days");
    else if(n==9)
    printf("30 days");
    else if(n==10)
    printf("31 days");
    else if(n==11)
    printf("30 days");
    else if(n==12)
    printf("31 days");
    else
    printf("Enter valid number between 1-12");
    

}
